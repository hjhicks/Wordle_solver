/* Dark Theme */
QWidget {
    background-color: #232629;
    color: #f8f8f2;
}
QPushButton {
    background-color: #44475a;
    color: #f8f8f2;
    border-radius: 5px;
    padding: 5px 10px;
}
QLineEdit, QTextEdit {
    background-color: #282a36;
    color: #f8f8f2;
    border: 1px solid #44475a;
    border-radius: 3px;
}
