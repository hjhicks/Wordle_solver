/* Blue Theme */
QWidget {
    background-color: #e6f0fa;
    color: #1a237e;
}
QPushButton {
    background-color: #90caf9;
    color: #1a237e;
    border-radius: 5px;
    padding: 5px 10px;
}
QLineEdit, QTextEdit {
    background-color: #bbdefb;
    color: #1a237e;
    border: 1px solid #90caf9;
    border-radius: 3px;
}
